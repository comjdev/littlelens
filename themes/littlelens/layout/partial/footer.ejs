			<div>
				<h2 class="mb-6 text-sm font-semibold text-gray-800 uppercase">
					Little Lens
				</h2>
				<ul class="text-little-lens-light-400">
					<li class="mb-4">
						<a href="/about" class="hover:underline">About</a>
					</li>
					<li class="mb-4">
						<a href="/how-it-works" class="hover:underline">How it works</a>
					</li>
					<li class="mb-4">
						<a href="/blog" class="hover:underline">Blog</a>
					</li>
				</ul>
			</div>
			<div>
				<h2 class="mb-6 text-sm font-semibold text-gray-800 uppercase">Help</h2>
				<ul class="text-little-lens-light-400">
					<li class="mb-4">
						<a href="/childcare-centre-faqs" class="hover:underline"
							>Info for Centres</a
						>
					</li>
					<li class="mb-4">
						<a href="/families-faqs" class="hover:underline"
							>Info for Parents</a
						>
					</li>
					<li class="mb-4">
						<a
							href="https://www.facebook.com/Gillian.Juergens.Photography/"
							target="_blank"
							class="hover:underline"
							>Facebook</a
						>
					</li>
					<li class="mb-4">
						<a href="/contact" class="hover:underline">Contact</a>
					</li>
				</ul>
			</div>
			<div>
				<h2 class="mb-6 text-sm font-semibold text-gray-800 uppercase">Blog</h2>
				<ul class="text-little-lens-light-400">
					<% site.posts.sort('date', -1).limit(3).forEach(function(post) { %>
					<li class="mb-4">
						<a href="/<%= post.path %>" class="hover:underline"
							><%= post.title %></a
						>
					</li>
					<% }) %>
				</ul>
			</div>
			<div>
				<h2 class="mb-6 text-sm font-semibold text-gray-800 uppercase">
					Legal
				</h2>
				<ul class="text-little-lens-light-400">
					<li class="mb-4">
						<a href="#" class="hover:underline">Privacy Policy</a>
					</li>
					<li class="mb-4">
						<a href="#" class="hover:underline">Terms</a>
					</li>
				</ul>
			</div>

			<div>
				<h2 class="mb-6 text-sm font-semibold text-gray-800 uppercase">
					Suburbs
				</h2>

				<ul class="text-little-lens-light-400">
					<li class="mb-4">
						<a class="hover:underline" href="/suburbs"> All Suburbs</a>
					</li>
					<% const suburbPages = site.pages .filter(page => page.layout ===
					'suburb') .sort((a, b) => (a.suburb || '').localeCompare(b.suburb ||
					'')) .slice(0, 3); suburbPages.forEach(function(page) { %>
					<li class="mb-4">
						<a class="hover:underline" href="<%= url_for(page.path) %>">
							<%= page.suburb %>
						</a>
					</li>
					<% }); %>
				</ul>
			</div>
