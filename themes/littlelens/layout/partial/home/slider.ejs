<!-- Slider main container -->
<div class="swiper w-full mb-10">
	<!-- Additional required wrapper -->
	<div class="swiper-wrapper">
		<!-- Slides -->
		<% for (let i = 1; i < 13; i++) { %>
		<div class="swiper-slide">
			<figure>
				<img
					class="slide-image"
					src="<%= 'img/photo-' + i + '.jpg' %>"
					alt="Childcare Photographer Melbourne"
				/>
			</figure>
		</div>
		<% } %>
	</div>
</div>
